                            </a>
                        {% endif %}
                        {% if gallery | length > 9 %}
                            {% for img in gallery[9:] %}
                                <a href="{{ img['src'] }}" class="swipebox ui-link" data-size="{{ img['size'] }}" imgId="{{ loop.index + 9 }}"></a>
                            {% endfor %}
                        {% endif %}
                    </div>
                </div>
            </div>
