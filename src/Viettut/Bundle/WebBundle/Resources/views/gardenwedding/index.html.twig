            <h2 class="title"><span>Album Ảnh</span></h2>
            <div class="ul-body">
                <div class="gal_list">
                    <div class="col-5">
                        {% for img in first %}
                            <a href="{{ img['src'] }}" class="swipebox" data-size="{{ img['size'] }}" imgId="{{ loop.index }}">
                                <div class="ui-block-{{ loop.index }}" style="background:url('{{ img['src'] }}') center center no-repeat;background-size:cover;"></div>
                            </a>
                        {% endfor %}
                    </div>
                    {% if second | length > 0 %}
                        <div class="col-4">
                            {% for img in second %}
                                <a href="{{ img['src'] }}" class="swipebox" data-size="{{ img['size'] }}" imgId="{{ loop.index + 5}}">
                                    <div class="ui-block-{{ loop.index }}" style="background:url('{{ img['src'] }}') center center no-repeat;background-size:cover;"></div>
                                </a>
                            {% endfor %}
                        </div>
                    {% endif %}
                    {% if rest | length > 0 %}
                        {% for img in rest %}
                            <a href="{{ img['src'] }}" class="swipebox" data-ajax="false" data-size="{{ img['size'] }}" imgId="{{ loop.index + 9}}">
                                {% if loop.index == 1 %}
                                    <p class="gal_more"><span>more</span></p>
                                {% endif %}
                            </a>
                        {% endfor %}
                    {% endif %}
                </div>
            </div>
        </div>

