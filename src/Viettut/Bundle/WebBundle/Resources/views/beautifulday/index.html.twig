                        </li>
                    </ul>

                </div>
            </div>

            <!-- gallery -->
            <div class="gallery_wrap" id="sk_gallery" data-pswp-uid="1">
                <h2 class="title">ALBUM ẢNH</h2>
                <div class="ul-body">
                    <div class="gal_list">
                        <div class="col-4">
                            {% for img in gallery[0:4] %}
                                <a href="{{ img['src'] }}" class="swipebox ui-link" data-size="{{ img['size'] }}" imgId="{{ loop.index }}">
                                    <div class="ui-block-{{ loop.index }}"
                                         style="background:url('{{ img['src'] }}') center center no-repeat;background-size:cover;">
                                    </div>
                                </a>
                            {% endfor %}
                        </div>
                        {% if gallery | length > 4 %}
                            <div class="col-4">
                                {% for img in gallery[4:4] %}
                                    <a href="{{ img['src'] }}" class="swipebox ui-link" data-size="{{ img['size'] }}" imgId="{{ loop.index + 4}}">
                                        <div class="ui-block-{{ loop.index }}"
                                             style="background:url('{{ img['src'] }}') center center no-repeat;background-size:cover;">
                                        </div>
                                    </a>
                                {% endfor %}
                            </div>
                        {% endif %}

                        {% if gallery | length > 8 %}
                            <a href="{{ gallery[8]['src'] }}"
                               class="swipebox ui-link" data-ajax="false" data-size="{{ gallery[8]['size'] }}" imgId="9">
