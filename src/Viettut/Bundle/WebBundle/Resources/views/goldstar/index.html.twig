

<!DOCTYPE html>
<html lang="vi">
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# website: http://ogp.me/ns/website#">

    <meta property="og:type" content="website"/>
    <meta property="og:url" content="http://thiepdo.com/gold-star"/>
    <meta property="og:description" content="{{ description }}"/>

    <meta property="og:title" content="{{ name }}" />
    <meta property="og:image" content="/bundles/viettutweb/img/templates/goldstar/9b854c77bd8bd11be8cfd28c7240e73e.jpg" />
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="robots" content="noindex" />

    <link href="/bundles/viettutweb/img/thiepdo_120x120.png" sizes="120x120" rel="apple-touch-icon">
    <link rel="icon" sizes="144x144" href="/bundles/viettutweb/img/thiepdo_144x144.png">
    <link rel="icon" sizes="64x64" href="/bundles/viettutweb/img/thiepdo_64x64.png">

    <title>{{ name }}</title>

    <link rel="stylesheet" type="text/css" href="{{ asset('css/libraries.min.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('css/goldstar/app.css') }}" />
    <link href='http://fonts.googleapis.com/css?family=Vidaloka' rel='stylesheet' type='text/css'>

</head>
<body>
<div id="coordinates" data-lon="{{ lon }}" data-lat="{{ lat }}" data-title="{{ data['place'] }}"
     data-home-lon="{{ homeLon }}" data-home-lat="{{ homeLat }}" data-home-title="{% if forGroom %}Nhà Trai{% else %}Nhà Gái{% endif %}"></div>
<div id="card_id" data-id="{{ id }}"></div>
<div data-role="page" id="skin_page">
    <!-- panel-groom -->
    <div data-role="panel" id="panel_groom" class="leftpanel" data-position="left" data-display="push" data-position-fixed="true" data-swipe-close="true" data-theme="b" style="background:#f3ebd3 url('/bundles/viettutweb/img/templates/goldstar/bg_groom.jpg') center top no-repeat; background-size:100%; z-index:9999;">
        <div class="groom-wrap">
            <ul>
                <a href="tel:{{ data['groom_phone'] }}"><li class="ico5">Gọi điện</li></a>
                <a href="sms:{{ data['groom_phone'] }}"><li class="ico6">Nhắn tin</li></a>
                {% if 'groom_facebook' in data|keys %}
                    <a href="{{ data['groom_facebook'] }}" target="_blank" title="Facebook">
                        <li class="ico7">Facebook</li>
                    </a>
                {% else %}
                    <a href="http://facebook.com/thiepdodigital" target="_blank" title="Facebook">
                        <li class="ico7">Facebook</li>
                    </a>
                {% endif %}
                {% if 'groom_instagram' in data|keys %}
                    <a href="{{ data['groom_instagram'] }}" target="_blank" title="Instagram">
                        <li class="ico_insta">Instagram</li>
                    </a>
                {% else %}
                    <a href="http://instagram.com/thiepdodigital" target="_blank" title="Instagram">
                        <li class="ico_insta">Instagram</li>
                    </a>
                {% endif %}
            </ul>
            <p class="btn_go_snsbbs">Lưu bút</p>
        </div>
    </div>
    <!--// panel-groom -->
    <!-- panel-bride -->
    <div data-role="panel" id="panel_bride" class="righttpanel" data-position="right" data-display="push" data-position-fixed="true" data-swipe-close="true" data-theme="a" style="background:#f3ebd3 url('/bundles/viettutweb/img/templates/goldstar/bg_bride.jpg') center top no-repeat; background-size:100% auto; z-index:9999;">
        <div class="bride-wrap">
            <ul>
                <a href="tel:{{ data['bride_phone'] }}"><li class="ico1">Gọi điện</li></a>
                <a href="sms:{{ data['bride_phone'] }}"><li class="ico2">Nhắn tin</li></a>
                {% if 'bride_facebook' in data|keys %}
                    <a href="{{ data['bride_facebook'] }}" target="_blank" title="Facebook">
                        <li class="ico3">Facebook</li>
                    </a>
                {% else %}
                    <a href="http://facebook.com/thiepdodigital" target="_blank" title="Facebook">
                        <li class="ico3">Facebook</li>
                    </a>
                {% endif %}
                {% if 'bride_instagram' in data|keys %}
                    <a href="http://instagram.com/{{ data['bride_instagram'] }}" target="_blank" title="Instagram">
                        <li class="ico_insta">Instagram</li>
                    </a>
                {% else %}
                    <a href="http://instagram.com/thiepdodigital" target="_blank" title="Instagram">
                        <li class="ico_insta">Instagram</li>
                    </a>
                {% endif %}
            </ul>
            <p class="btn_go_snsbbs">Lưu bút</p>
        </div>
    </div>
    <!--// panel-bride -->
    <!-- panel-menu -->
    <div data-role="panel" id="right-panel" data-position="right" data-display="push" style="z-index:9999;">
        <div class="menu-wrap ui-panel-inner">
            <ul data-role="listview" data-icon="false">
                <li class="menu_scrolls" data-rel="close"><span>TRANG CHỦ</span></li>
                <li class="menu_scrolls" data-rel="close" id="p_topinfo"><span>LỜI TỰA</span></li>
                <li class="menu_scrolls" data-rel="close" id="p_gallery"><span>ALBUM ẢNH</span></li>
                <li class="menu_scrolls" data-rel="close" id="p_media"><span>VIDEO</span></li>
                <li class="menu_scrolls" data-rel="close" id="p_snsbbs"><span>LƯU BÚT</span></li>
                <li class="menu_scrolls" data-rel="close" id="p_map"><span>ĐỊA ĐIỂM</span></li>
            </ul>
        </div>
    </div>
    <!--// panel-menu -->

    <div class="lnb slideDown">
        <div class="lnb_title">JUST MARRIED!</div>
        <ul>
            <li class="menu1 menu_scrolls"><span>TRANG CHỦ</span></li>
            <li class="menu1 menu_scrolls on" id="m_topinfo"><span>LỜI TỰA</span></li>
            <li class="menu2 menu_scrolls" id="m_gallery"><span>ALBUM ẢNH</span></li>
            <li class="menu3 menu_scrolls" id="m_media"><span>VIDEO</span></li>
            <li class="menu4 menu_scrolls" id="m_snsbbs"><span>LƯU BÚT</span></li>
            <li class="menu5 menu_scrolls" id="m_map"><span>ĐỊA ĐIỂM</span></li>
        </ul>
    </div>
    <div class="lnb_mobile"><a href="#right-panel"><img src="/bundles/viettutweb/img/templates/goldstar/lnb_mobile.png" alt=""></a></div>

    <!-- header -->
    <div id="sk_topinfo" class="header">
        <div class="header_bg" style="background:url('/bundles/viettutweb/img/templates/goldstar/9b854c77bd8bd11be8cfd28c7240e73e.jpg') CENTER CENTER no-repeat; background-size:cover;"></div>
        <div class="header_content">
            <div class="header_photo">
                <p style="background:url('/bundles/viettutweb/img/templates/goldstar/9b854c77bd8bd11be8cfd28c7240e73e.jpg') CENTER CENTER no-repeat; background-size:cover;"></p>
            </div>
            <div class="header_title">
                <p class="hd_name">
                    {% if forGroom %}
                        {{ data['groom_name'] }}<span>và</span>{{ data['bride_name'] }}
                    {% else %}
                        {{ data['bride_name'] }}<span>và</span>{{ data['groom_name'] }}
                    {% endif %}
                </p>

                <p class="hd_date">{{ date | long_date }}, hồi {{ date | date('H:i') }}</p>
                <p class="hd_loca">(tức {{ dateAl }})</p>
                <p class="hd_loca">{{ data['place'] }}</p>
            </div>
            <div class="header_panel">
                {% if forGroom %}
                    <p class="groom slideLeft"><a href="#panel_groom">CHÚ RỂ</a></p>
                    <p class="bride slideRight"><a href="#panel_bride">CÔ DÂU</a></p>
                {% else %}
                    <p class="bride slideRight"><a href="#panel_groom">CHÚ RỂ</a></p>
                    <p class="groom slideLeft"><a href="#panel_bride">CÔ DÂU</a></p>
                {% endif %}
            </div>
        </div>
        <div class="scroll slideUp"><span>Trượt xuống</span></div>
    </div>
    <!--// header -->

    <!-- greeting -->
    <div class="greeting_wrap">
        <div class="greeting_textbox">
            <h2 class="title"><img src="/bundles/viettutweb/img/templates/goldstar/greeting_title.png" alt="WEDDING INVITATION"></h2>
            <p>{{ data['greeting'] }}</p>
            {% if forGroom %}
                <table class="greeting_name">
                    <thead>
                    <tr>
                        <th>Nhà Trai</th>
                        <th>Nhà Gái</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td class="gn2">{{ data['groom_fa_name'] }}</td>
                        <td class="gn1">{{ data['bride_fa_name'] }}</td>
                    </tr>
                    <tr>
                        <td class="gn2">{{ data['groom_mo_name'] }}</td>
                        <td class="gn1">{{ data['bride_mo_name'] }}</td>
                    </tr>
                    </tbody>
                </table>
            {% else %}
                <table class="greeting_name">
                    <thead>
                    <tr>
                        <th>Nhà Gái</th>
                        <th>Nhà Trai</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td class="gn2">{{ data['bride_fa_name'] }}</td>
                        <td class="gn1">{{ data['groom_fa_name'] }}</td>
                    </tr>
                    <tr>
                        <td class="gn2">{{ data['bride_mo_name'] }}</td>
                        <td class="gn1">{{ data['groom_mo_name'] }}</td>
                    </tr>
                    </tbody>
                </table>
            {% endif %}
        </div>
    </div>
    <!--// greeting -->


    <!-- gallery -->
    <div class="gallery_wrap ui-cont-b" id="sk_gallery">
        <h2 class="title title_pink"><span>ALBUM ẢNH</span></h2>
        <div class="ul-body">
            <div class="gal_list2">
                <div class="gal_size"></div>
                {% for img in gallery %}
                    <a href="{{ img['src'] }}" class="swipebox" data-ajax="false" data-size="{{ img['size'] }}" imgId="{{ loop.index }}">
                        {% if (gallery|length == loop.index) or (loop.index >= 10)  %}
                            <div class="gal_more"><span>more</span></div>
                        {% else %}
                            <div class="gal_frame"><img src="{{ img['src'] }}"></div>
                        {% endif %}
                    </a>
                {% endfor %}
            </div>
        </div>
    </div>
    <!--// gallery -->

    {% if isTemplate %}
        <!-- movie -->
        <div class="movie_wrap ui-cont-b">
            <h2 class="title hidden">VIDEO</h2>
            <div class="ul-body">
                <div class="movie_container movie1">
                    <ul>
                        <li class="iframe-container">
                            <iframe
                                    id="movie"
                                    width="100%"
                                    src="http://www.youtube.com/embed/Q9aUb6FJdhk?showinfo=0&autohide=1&rel=0&vq=hd1080"
                                    height="100%"
                                    frameborder="0"
                                    allowfullscreen>
                            </iframe>
                        </li>
                    </ul>
                </div>

                <div class="lovestory">
                    <p class="lovestory_img"><img src="/bundles/viettutweb/img/templates/goldstar/movie_text3.png" alt="Love Story"></p>
                    <p class="lovestory_txt">Now at last they were beginning Chapter One<br>
                        of the Great Story no one on earth has ever read.<br>
                        which goes on forever :<br>
                        in which every chapter is better than the one before.</p>
                </div>
            </div>
        </div>
        <!--// movie -->
    {% else %}
        {% if embed | length > 0 %}
            <!-- movie -->
            <div class="movie_wrap ui-cont-b">
                <h2 class="title hidden">VIDEO</h2>
                <div class="ul-body">
                    <div class="movie_container movie1">
                        <ul>
                            <li class="iframe-container">
                                {{ embed | raw }}
                            </li>
                        </ul>
                    </div>

                    <div class="lovestory">
                        <p class="lovestory_img"><img src="/bundles/viettutweb/img/templates/goldstar/movie_text3.png" alt="Love Story"></p>
                        <p class="lovestory_txt">Now at last they were beginning Chapter One<br>
                            of the Great Story no one on earth has ever read.<br>
                            which goes on forever :<br>
                            in which every chapter is better than the one before.</p>
                    </div>
                </div>
            </div>
            <!--// movie -->
        {% endif %}
    {% endif %}


    <!-- guestbook -->
    <div class="guestbook_wrap ui-cont-b" id="sk_snsbbs">
        <h2 class="title title_pink"><span>LƯU BÚT</span></h2>

        <div class="ul-body col-3">
            <div class="gb_msg">Hãy chúc mừng cho ngày vui của cô dâu chú rể !</div>
            <div id="div_snsbbs">
                {% if isTemplate %}
                <div class="snsbbs_sections">
                    <div class="ui-body-d">
                        <div class="gb_cont_box btn_go_snsbbs">
                            <div class="pro_info">
                                <div class="pro_img">
                                    <img src="/bundles/viettutweb/img/templates/gardenwedding/img_profile.png" alt="profile picture" />
                                </div>
                                <div class="pro_txt">
                                    <p><strong>Sunny</strong></p>
                                    <p class="date">2016-11-04 09:03</p>
                                </div>
                            </div>
                            <ul class="gb_cont btn_go_snsbbs">
                                <li class="comm_txt">
                                    <p>
                                        I sincerely congratulate the marriage of the two! I really married when I saw two people and I married like this !!!! Really Too Much Too Much Too Pretty Couple Happily Long To Live Ham Frying ~~~
                                    </p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="snsbbs_sections ">
                    <div class="ui-body-d">
                        <div class="gb_cont_box btn_go_snsbbs">
                            <div class="pro_info">
                                <div class="pro_img">
                                    <img src="/bundles/viettutweb/img/templates/gardenwedding/img_profile.png" alt="profile picture" />
                                </div>
                                <div class="pro_txt">
                                    <p><strong>Windy</strong></p>
                                    <p class="date">2016-10-05 15:36</p>
                                </div>
                            </div>
                            <ul class="gb_cont btn_go_snsbbs">
                                <li class="comm_txt">
                                    <p>Chúc mừng đôi trẻ !</p>
                                    <div class="emo-type-a emo08"></div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="snsbbs_sections ">
                    <div class="ui-body-d">
                        <div class="gb_cont_box btn_go_snsbbs">
                            <div class="pro_info">
                                <div class="pro_img">
                                    <img src="/bundles/viettutweb/img/templates/gardenwedding/img_profile.png" alt="profile picture" />
                                    {#<div class="pro-sns instagram"><span></span></div>#}
                                </div>
                                <div class="pro_txt">
                                    <p><strong>Song Song</strong></p>
                                    <p class="date">2016-07-19 18:12</p>
