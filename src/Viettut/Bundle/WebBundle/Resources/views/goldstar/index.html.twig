        {% else %}
            <a href="{{ path('guest_book_page', {hash: hash}) }}"><div class="gb_btn btn_go_snsbbs"><span>Chúc mừng</span></div></a>
        {% endif %}
    </div>
    <!--// guestbook -->

    <!-- location -->
    <div class="location_wrap ui-cont-b" id="sk_map">
        <h2 class="title title_gold"><span>ĐỊA ĐIỂM</span></h2>
        <div class="ul-body">
            <div class="col-2">
                <div class="ui-block-a">
                    <ul class="lo_tab_wrap">
                        <li class="tab_btn wh locationtabs on" id="tabs_wedding" style="cursor: pointer;">Lễ Thành Hôn</li>
                        <li class="tab_btn rh locationtabs" id="tabs_party" style="cursor: pointer;">Tiệc cưới</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="ul-body locationdivs" id="div_wedding">
            <div class="lo_box col-2">
                <div class="ui-block-a">
                    <h3 class="lo_title">{{ data['home'] }}</h3>
                    <div class="lo_map">
                        <div id="wedding_pmap_canvas"  class="not_view_panel" style="width:100%;height:500px;"></div>
                    </div>
                    <div class="nav-wrap">
                        <div class="ui-grid-a nav-m">
                            <form action='http://maps.google.com/maps' method='get' target='_blank' id="getHomePath">
                                <input type='text' name='saddr' placeholder="địa chỉ của bạn"  style="height: 25px;width:300px;"/>
                                <input type='hidden' name='daddr' value='{{ data['home'] }}' />
                            </form>
                            <div class="nav-1">
                                <div class="ui-body-d ui-btn-nav wedding_kimnavi">
                                    <span class="ui-btn ui-icon-nav1 ui-btn-icon-left ui-btn-b ui-corner-all ui-shadow ui-custom-btn fc-fff" onclick="getHomePath()">CHỈ ĐƯỜNG</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="nav_info">※ Gõ chính xác địa chỉ của bạn vào ô ở trên.</div>
                </div>
                <div class="ui-block-b">

                    <table class="lo_container">
                        <tr>
                            <th class="lo_date">Thời gian</th>
                            <td>{{ weddingDate | long_date }}, hồi {{ weddingDate | date('H:i') }}</td>
                        </tr>
                        <tr>
                            <th class="lo_loca">{% if forGroom %}Nhà Trai{% else %}Nhà Gái{% endif %}</th>
                            <td>{{ data['home'] }}</td>
                        </tr>
                        <tr>
                            <th class="lo_tel">Hướng dẫn</th>
                            <td>{{ data['home_info'] }}</td>
                        </tr>
                        <tr>
                            <th class="lo_tel">Liên hệ</th>
