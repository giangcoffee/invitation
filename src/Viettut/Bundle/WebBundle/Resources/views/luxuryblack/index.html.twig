                    var img_el = imageEl.getElementsByTagName("img");
                    var exist_img = img_el.length > 0 ? true : false;
                    var src = imageEl.href;
                    var is_swipe_image = imageEl.className.indexOf("swipebox") > -1 ? true : false;
