        (function ($) {
            $.fn.onScrollTo = function (focusInFn, focusOutFn) {
                var $this = this;
                $(document).scroll(function () {
                    var y = $(this).scrollTop();
                    if (y >= $this.position().top) {
                        if (focusInFn) focusInFn();
                    } else {
                        if (focusOutFn) focusOutFn();
