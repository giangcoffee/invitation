<body class="ui-mobile-viewport ui-overlay-a">
<div id="coordinates" data-lon="{{ lon }}" data-lat="{{ lat }}" data-title="{{ data['place'] }}"
     data-home-lon="{{ homeLon }}" data-home-lat="{{ homeLat }}" data-home-title="{% if forGroom %}Nhà Trai{% else %}Nhà Gái{% endif %}"></div>
<div id="card_id" data-id="{{ id }}"></div>
<div data-role="page" id="skin_page" data-url="skin_page" tabindex="0" class="ui-page ui-page-theme-a ui-page-active">
    <!-- panel-groom -->
    <div data-role="panel" id="panel_groom"
         class="righttpanel ui-panel ui-panel-position-right ui-panel-display-push ui-panel-closed ui-body-b ui-panel-fixed ui-panel-animate"
         data-position="right" data-display="push" data-position-fixed="true" data-swipe-close="true" data-theme="b"
         style="background:#fff url('/bundles/viettutweb/img/templates/beautifulday/bg_groom.jpg') center top no-repeat; background-size:100%; z-index:9999;">
        <div class="ui-panel-inner">
            <div class="groom-wrap">
                <ul>
                    <a href="tel:{{ data['groom_phone'] }}" class="ui-link">
                        <li class="ico5">Gọi điện</li>
                    </a>
                    <a href="sms:{{ data['groom_phone'] }}" class="ui-link">
                        <li class="ico6">Nhắn tin</li>
                    </a>
                    {% if data['groom_facebook'] is empty %}
                        <a href="http://facebook.com/thiepdodigital" target="_blank" class="ui-link">
                            <li class="ico7">Facebook</li>
                        </a>
                    {% else %}
                        <a href="{{ data['groom_facebook'] }}" target="_blank" class="ui-link">
                            <li class="ico7">Facebook</li>
                        </a>
                    {% endif %}
                    {% if data['groom_instagram'] is empty %}
                        <a href="http://instagram.com/thiepdodigital" target="_blank" class="ui-link">
                            <li class="ico_insta">Instagram</li>
                        </a>
                    {% else %}
                        <a href="{{ data['groom_instagram'] }}" target="_blank" class="ui-link">
                            <li class="ico_insta">Instagram</li>
                        </a>
                    {% endif %}

                </ul>
                <p class="btn_go_snsbbs" style="cursor: pointer;">Lưu bút</p>
            </div>
        </div>
    </div>
    <!--// panel-groom -->
